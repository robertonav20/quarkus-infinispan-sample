{
  "simpleCache": {
    "distributed-cache": {
      "mode": "SYNC",
      "remote-timeout": "17500",
      "statistics": true,
      "encoding": {
        "key": {
          "media-type": "application/x-protostream"
        },
        "value": {
          "media-type": "application/x-protostream"
        }
      },
      "locking": {
        "concurrency-level": "1000",
        "acquire-timeout": "15000",
        "striping": false
      },
      "state-transfer": {
        "timeout": "60000"
      }
    }
  }
}